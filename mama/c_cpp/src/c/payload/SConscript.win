import os
Import('env')
env = env.Clone()

modules = []

if env.get('with_enterprise',None) == True:
    modules.append('wombatmsg')
    modules.append('wcache')

if 'avis' in env['middleware']:
    modules.append('avismsg')

for m in modules:
   env.SConscript( os.path.join(m,'SConscript.win'), 'env' )
