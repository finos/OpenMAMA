<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <AssemblyName>mamaunittestscs</AssemblyName>
        <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
        <TargetFramework>netcoreapp2.1</TargetFramework>
        <Title>OpenMAMA</Title>
        <Description>OpenMAMA API C# Unit Tests</Description>
        <Configuration>OpenMAMA API C# Unit Tests</Configuration>
        <Product>OpenMAMA API C# Unit Tests</Product>
